<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#494949"
    tools:context=".controladores.inventario.AjustesUsuario">


    <Button
        android:id="@+id/buttonCambiarContra"
        android:layout_width="284dp"
        android:layout_height="104dp"
        android:layout_marginTop="203dp"
        android:backgroundTint="#2D2F31"
        android:fontFamily="@font/new_rocker"
        android:shadowColor="#000000"
        android:text="Cambiar ContraseÃ±a"
        android:textColor="#FFEB3B"
        android:textSize="36sp"
        app:cornerRadius="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        app:strokeColor="#313435"
        app:strokeWidth="5px" />

    <Button
        android:id="@+id/buttonCambiarDatos"
        android:layout_width="284dp"
        android:layout_height="67dp"
        android:layout_marginTop="76dp"
        android:layout_marginBottom="60dp"
        android:backgroundTint="#2D2F31"
        android:fontFamily="@font/new_rocker"
        android:shadowColor="#2D2F31"
        android:text="Cambiar Datos"
        android:textColor="#FEEA3B"
        android:textSize="36sp"
        app:cornerRadius="20sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonCambiarContra"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        app:strokeColor="#313435"
        app:strokeWidth="5px" />

    <Button
        android:id="@+id/buttonSalir"
        android:layout_width="284dp"
        android:layout_height="67dp"
        android:layout_marginTop="70dp"
        android:backgroundTint="#FFEB3B"
        android:fontFamily="@font/new_rocker"
        android:shadowColor="#000000"
        android:text="Cerrar Sesion"
        android:textColor="#131313"
        android:textSize="36sp"
        app:cornerRadius="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCambiarContra"
        app:strokeColor="#000000"
        app:strokeWidth="5px" />

    <Button
        android:id="@+id/buttonReturn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="12dp"
        android:backgroundTint="#00FFFFFF"
        android:fontFamily="@font/new_rocker"
        android:text="Return"
        android:textColor="#FFEB3B"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="174dp"
        android:layout_height="146dp"
        android:layout_marginTop="72dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ajustes" />

</androidx.constraintlayout.widget.ConstraintLayout>